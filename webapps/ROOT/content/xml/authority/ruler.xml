<TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title xml:lang="en">Authority list of rulers in ECG</title>
            </titleStmt>
            <publicationStmt><ab/></publicationStmt>
            <sourceDesc><ab/></sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div type="commentary">
                <listPerson type="rome">
                    <head xml:lang="en">Emperors of Rome/Byzantium</head>
                    <head xml:lang="ka">რომისა და ბიზანტიის იმპერატორები</head>
                    
                    <person xml:id="vespasianus">
                        <persName xml:lang="en">Vespasian</persName>
                        <persName xml:lang="ka">ვესპასიანე</persName>
                        <persName xml:lang="la">Titus Flavius Caesar Vespasianus Augustus</persName>
                        <persName xml:lang="grc">Αὐτοκράτωρ Οὐεσπασιανὸς Καῖσαρ Σεβαστός</persName>
                        <floruit>69-79</floruit>
                        <idno type="wp">https://en.wikipedia.org/wiki/Vespasian</idno>
                        <idno type="wp-ru">https://ru.wikipedia.org/wiki/Веспасиан</idno>
                    </person>
                   
                  
                  
                </listPerson>
                
                <listPerson type="ka">
                    <head xml:lang="en">Rulers of the Gerogian Kingdom</head>
                    <head xml:lang="ka">KA: საქართველოს მმართველები</head>
                    
                    <person xml:id="bagrat3"></person>
                    
                    <person xml:id="giorgi3">
                        <persName xml:lang="ka">გიორგი III</persName>
                        <persName xml:lang="hy">թագաւոր Գէորգի</persName>
                        <floruit>1156-1184</floruit>
                        <idno type="wp">https://en.wikipedia.org/wiki/George_III_of_Georgia</idno>
                        <idno type="wp-ka">https://ka.wikipedia.org/wiki/გიორგი_III_(საქართველოს_მეფე)</idno>
                        <listBibl> 
                            <bibl></bibl>
                            <relation active="#giorgi3" name="father" passive="#tamari">
                            </relation>
                        </listBibl>
                    </person>
                    
                    <person xml:id="tamari">
                        <persName xml:lang="ka">თამარ მეფე</persName>
                        <persName xml:lang="hy">մեծ թագաւորին Թամարին՝</persName>
                        <persName xml:lang="en">Tamar of Georgia</persName>
                        <floruit>1184-1213</floruit>
                        <idno type="wp">https://en.wikipedia.org/wiki/Tamar_of_Georgia</idno>
                        <idno type="wp-ka">https://ka.wikipedia.org/wiki/თამარ_მეფე</idno>
                        <listBibl>
                            <bibl></bibl>
                            <relation active="#tamari" name="mother" passive="#lashagiorgi">
                            </relation>
                        </listBibl>
                    </person>
                    
                    <person xml:id="davit_s">
                        <persName xml:lang="ka">დავით სოსლანი</persName>
                        <persName xml:lang="hy"/>
                        <persName xml:lang="en"/>
                        <floruit/>
                        <idno type="wp">https://en.wikipedia.org/wiki/Tamar_of_Georgia</idno>
                        <idno type="wp-ka">https://ka.wikipedia.org/wiki/თამარ_მეფე</idno>
                        <bibl>
                            <listRelation>
                            <relation active="#tamari" name="wife" passive="#davit_s">
                            </relation>
                        </listRelation>
                        </bibl>
                    </person>
                    
                    <person xml:id="lashagiorgi">
                        <persName xml:lang="ka">გიორგი IV ლაშა</persName>
                        <persName xml:lang="hy">Գէորգ</persName>
                        <persName xml:lang="en">George IV of Georgia</persName>
                        <floruit>1213–1223</floruit>
                        <idno type="wp">https://en.wikipedia.org/wiki/George_IV_of_Georgia</idno>
                        <idno type="wp-ka">https://ka.wikipedia.org/wiki/გიორგი_IV_ლაშა</idno>
                    </person>
                    
                    <person xml:id="rusudan">
                        <persName xml:lang="ka">რუსუდან</persName>
                        <bibl>
                            <listRelation>
                                <relation active="#rusudan" name="sister" passive="#lashagiorgi">
                                    <desc xml:lang="ka"></desc>
                                </relation>
                            </listRelation>
                        </bibl>
                    </person>         
                    
                    <person xml:id="davitVI">
                        <persName xml:lang="ka">დავით VI ნარინი</persName>
                    </person>
                    
                    <person xml:id="davitVII">
                        <persName xml:lang="ka">დავით VII</persName>
                        <bibl>
                            <listRelation>
                                <relation active="#davitVII" name="son" passive="#lashagiorgi">
                                    <desc xml:lang="ka"></desc>
                                </relation>
                            </listRelation>
                        </bibl>
                    </person>
                    

                    
                    <person xml:id="zaqariaM">
                        <persName xml:lang="ka">ზაქარია მხარგრძელი</persName>
                        <persName xml:lang="hy">Զաքարէի</persName>
                        <listBibl>
                            <bibl></bibl>
                            <relation active="#zaqariaM" name="brother" passive="#ivaneM">                         
                                <desc xml:lang="ka">ძმა ივანე მხარგრძელისა</desc>             
                            </relation>
                        </listBibl>
                    </person>
                    
                    <person xml:id="ivaneM">
                        <persName xml:lang="ka">ივანე მხარგრძელი</persName>
                        <persName xml:lang="hy">Իւանէ</persName>
                    </person>
                    
                    <person xml:id="sargis1">
                        <persName xml:lang="ka">იშხანი სარგისი</persName>
                        <persName xml:lang="hy">իշխան Սարգիս</persName>
                    </person>
                    
                    <person xml:id="davit1">
                        <persName xml:lang="ka">დავით</persName>
                        <persName xml:lang="hy">Դաւիթ</persName>
                    </person>
                    
                    <person xml:id="shotriki">
                        <persName xml:lang="ka">შოთრიკი</persName>
                        <persName xml:lang="hy">Շոթոռիկ</persName>
                    </person>
                    
                    <person xml:id="shanshe1">
                        <persName xml:lang="ka">შანშე</persName>
                        <persName xml:lang="hy">Շահանշահ</persName>
                    </person>
                    
                    <person xml:id="avag1">
                        <persName xml:lang="ka">ავაგ</persName>
                        <persName xml:lang="hy">Աւագ</persName>
                    </person>
                    
                    <person xml:id="giorgi_d">
                        <persName xml:lang="ka">მეფე გიორგი</persName>
                        <persName xml:lang="hy">թագաւոր Գէորգի</persName>
                        <listBibl>
                            <bibl>შანშე დარჩიაშვილი, ძიებანი ქართულ-სომხური ურთიერთობების ისტორიიდან თბილისი 2010</bibl>                        
                            <relation active="#giorgi_d" name="son" passive="#davit2">
                                <desc xml:lang="ka">ვაჟი დავით მეფისა</desc>
                            </relation>
                        </listBibl>
                    </person>
                    
                    <person xml:id="davit2">
                        <persName xml:lang="ka">მეფე დავითი</persName>
                        <persName xml:lang="hy">թագաւոր Դաւիթ</persName>
                    </person>
                    
                    
                </listPerson>
                
              
              <listPerson type="persian">
                  <person xml:id="peroz">
                      <persName xml:lang="ka">პეროზ მეფე</persName>
                  </person>
              </listPerson>
            </div>
        </body>
    </text>
</TEI>