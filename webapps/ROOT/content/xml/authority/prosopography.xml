    <text>
        <body>
            <div>
                <listPerson>
                    <head xml:lang="en">Persons of the ECG</head>
                    <head xml:lang="ka">პირები</head>
                    
                  
                    
                    <person xml:id="tikas">
                        <persName xml:lang="ka">ტიკასი</persName>
                        <persName xml:lang="en">Tikas</persName>
                        <persName xml:lang="grc">Τικᾶς</persName>
                        <floruit>IV-V AD</floruit>
                        <occupation xml:lang="ka"></occupation>
                        <occupation xml:lang="en"></occupation>
                        <bibl>
                            <listRelation>
                                <relation active="#tikas" name="son" passive="#pharanuses">
                                    <desc xml:lang="ka">ძმა პეტრესი</desc>
                                </relation>
                            </listRelation>
                        </bibl>
                    </person>
                    
                    <person xml:id="pharanuses">
                        <persName xml:lang="ka">აბბა ფარანუსესი</persName>
                        <persName xml:lang="en">Pharanuses</persName>
                        <persName xml:lang="grc">ἀββᾶ Φαρανούσης</persName>
                        <floruit>IV-V AD</floruit>
                        <occupation xml:lang="ka">ბერი, ἡσυχαστής</occupation>
                        <occupation xml:lang="en"></occupation>
                        <bibl>
                            <listRelation>
                                <relation active="#pharanuses" name="father" passive="#tikas">
                                    <desc>აბბა ტიკასისა</desc>
                                </relation>
                            </listRelation>
                        </bibl>
                    </person>
                    
                    <person xml:id="papriupas">
                        <persName xml:lang="ka">პაპრიუპასი</persName>
                        <persName xml:lang="en">Papriupas</persName>
                        <persName xml:lang="grc">Παπριοῦπας</persName>
                        <floruit>IV-V AD</floruit>
                        <occupation xml:lang="ka"></occupation>
                        <occupation xml:lang="en"></occupation>
                       <!--  <bibl>
                            <listRelation>
                                <relation active="" name="son" passive="">
                                    <desc></desc>
                                </relation>
                            </listRelation>
                        </bibl>
                       -->
                    </person>
                    
                    
                    <person xml:id="isdigerdes">
                        <persName xml:lang="ka">ისდიგერდესი</persName>
                        <persName xml:lang="en">Isdigerdes</persName>
                        <persName xml:lang="grc">Ἰσδιγέρδης</persName>
                        <floruit>IV-V AD</floruit>
                        <occupation xml:lang="ka"></occupation>
                        <occupation xml:lang="en"></occupation>
                        <!--  <bibl>
                            <listRelation>
                                <relation active="" name="son" passive="">
                                    <desc></desc>
                                </relation>
                            </listRelation>
                        </bibl>
                       -->
